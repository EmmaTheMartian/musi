let print_table = fn table, indent_level do
	let tabs = repeatstring('  ', indent_level)
	let tabs_indented = repeatstring('  ', indent_level + 1)
	println('{')
	table -> pairs() -> each(fn it do
		print(tabs_indented)
		print(it.key)
		print(' = ')
		let type = typeof(it.value)
		if type == 'table' do
			print_table(it.value, indent_level + 1)
		end else do
			println(tostring(it.value))
		end
	end)
	print(tabs)
	println('}')
end

let table = { }
table.test = 'Hello, World!'
table.users = {}
table.users."Emma the Martian" = {}
print_table(table, 0)

table.users."Emma the Martian".name = "Emma"
println(table.users)
println(table.users)
println(table.users."Emma the Martian")
