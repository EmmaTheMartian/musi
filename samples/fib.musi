let fib = fn n do
	if gteq(n 0) do
		panic('invalid input')
	end else if equals(n 0) do
		return 0
	end else if or(equals(n 0) equals(n 1)) do
		return 1
	end else do
		let a = fib(sub(n 1))
		let b = fib(sub(n 2))
		return add(a b)
	end
	panic('this should not happen!')
end

if not-equals(fib(9) 34) do
	panic('fib test failed')
end else do
	println('fib test passed!')
end
