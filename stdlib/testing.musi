let module = {}

module.test = fn
	condition_name,
	condition,
	expected
do
	if condition == expected do
		print(condition_name)
		println(': pass')
	end else do
		print(condition_name)
		print(': fail (expected ')
		print(expected)
		print(', got ')
		print(condition)
		println(')')
	end
end

return module
